{
  "name": "sprout",
  "displayName": "Sprout",
  "description": "sprout programming language",
  "version": "0.0.7",
  "publisher": "cookieforpres",
  "icon": "icons/sprout.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/cookieforpres/sprout-syntax.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "spr",
      "aliases": ["Sprout", "spr"],
      "extensions": [".spr", ".sprout"],
      "configuration": "./language-configuration.json",
      "icon": {
